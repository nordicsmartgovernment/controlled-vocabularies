@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix quantifier: <https://raw.githubusercontent.com/nordicsmartgovernment/controlled-vocabularies/refs/heads/main/QuantifierTypes/QuantifierTypes.ttl#> .

quantifier:scheme a skos:ConceptScheme ;
    dct:title "Extent Vocabulary" ;
    dct:description "A vocabulary describing extent categories (all, majority, alone)." .

quantifier:all a skos:Concept ;
    skos:inScheme quantifier:scheme ;
    skos:prefLabel "All of"@en ;
    skos:notation "all" ;
    skos:definition "Applies to the entire set or group."@en .

quantifier:majority a skos:Concept ;
    skos:inScheme quantifier:scheme ;
    skos:prefLabel "Majority of"@en ;
    skos:notation "majority" ;
    skos:definition "Applies to more than half of the set or group."@en .

quantifier:alone a skos:Concept ;
    skos:inScheme quantifier:scheme ;
    skos:prefLabel "Alone"@en ;
    skos:notation "alone" ;
    skos:definition "Applies to a single entity acting or standing by itself."@en .
