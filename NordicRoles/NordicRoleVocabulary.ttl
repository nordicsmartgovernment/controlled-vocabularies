@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix nsgrole: <https://raw.githubusercontent.com/nordicsmartgovernment/controlled-vocabularies/refs/heads/main/NordicRoles/NordicRoleVocabulary.ttl#> .
@prefix dct: <http://purl.org/dc/terms/> .

nsgrole:NordicRoleVocabulary a skos:ConceptScheme ;
    dct:title "Nordic Role Vocabulary"@en ;
    dct:description "Test version of the Nordic role vocabulary based on board- and company-related roles across Sweden, Norway, Finland, and Iceland."@en ;
    skos:prefLabel "Nordic Role Vocabulary (Test Version)"@en ;
   
    skos:hasTopConcept 
        nsgrole:MemberOfTheBoard,
        nsgrole:ChairmanOfTheBoard,
        nsgrole:DeputyMemberOfTheBoard,
        nsgrole:BoardObserver,
        nsgrole:ManagingDirector,
        nsgrole:DeputyManagingDirector,
        nsgrole:ExternalSignatory,
        nsgrole:SoleTrader,
        nsgrole:ProcurationHolder,
        nsgrole:Partner,
        nsgrole:LimitedPartner,
        nsgrole:OwnerMunicipality,
        nsgrole:ManagingShipowner,
        nsgrole:DesignatedRepresentative,
        nsgrole:GeneralPartner,
        nsgrole:Liquidator,
        nsgrole:DeputyLiquidator,
        nsgrole:BusinessManager,
        nsgrole:ContactPerson,
        nsgrole:Auditor,
        nsgrole:DeputyAuditor,
        nsgrole:Accountant,
        nsgrole:Actuary .


